#include <iostream>
using namespace std;

int main()
{
int a=8,b=3

//Addition operator
cout<<"a+b="<<(a+b)<<endl;

//Subtraction operator
cout<<"a-b="<<(a-b)<<endl;

//Multiplication operator
cout<<"a*b="<<(a*b)<<endl;

//Division operator
cout<<"a/b="<<(a/b)<<endl;

//Module operator
cout<<"a%b="<<(a%b)<<endl;

return 0;
}








